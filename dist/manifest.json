{
  "name": "Tetris Extension",
  "description": "Using ReactJS to build a Chrome Extension",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./obj-16x16.png",
    "32": "./obj-32x32.png",
    "128": "./obj-128x128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "<all_urls>"]
}
